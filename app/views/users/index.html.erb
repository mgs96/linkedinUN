<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>User id</th>
      <th>User name</th>
      <th>Password</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Semester</th>
      <th>Career</th>
      <th>Jobs</th>
      <th>Friends</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.user_name %></td>
        <td><%= user.password %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.semester %></td>
        <td><%= user.careers.try(:name) %></td>
        <td><%= user.jobs.try(:name) %></td>
        <td><%= user.friends.try(:first_name) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
